  "scripts": {
    "setup": "npm install && ionic state restore && npm install",
    "reset": "rm -rf node_modules platforms plugins && npm run setup",
    "webpack": "webpack --colors --display-error-details --mode=development",
    "webpack-release": "webpack --colors --display-error-details --mode=production",
    "build": "npm run webpack && ionic build",
    "release-build": "npm run webpack-release && ionic build --release --buildConfig build.json ios android",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -u",
    "increment": "bin/update-build-number.js",
    "release": "npm run lint && npm run test && npm run release-build",
    "verbose-build": "npm run lint && npm run test && webpack --debug --colors --display-modules --display-chunks --display-error-details --display-origins --display-reasons --env=development",
    "genymotion": "ionic run -slc android",
    "android": "ionic emulate -slc android",
    "ios": "ionic emulate -slc --address=localhost ios",
    "device": "ionic run --device -slc ios",
    "watch": "webpack --colors --display-error-details --watch --mode=development",
    "lint": "eslint . --ext .js"
  }
